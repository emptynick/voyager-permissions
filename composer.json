{
    "name": "emptynick/voyager-permissions",
    "description": "Permission system for Voyager II using spatie/laravel-permission",
    "keywords": ["voyager2-plugin", "voyager", "permission", "permissions", "roles", "auth", "authorization", "authorize"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Schweppe",
            "email": "info@cschweppe.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Emptynick\\Permissions\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Emptynick\\Permissions\\PermissionsServiceProvider"
            ]
        }
    },
    "require": {
        "spatie/laravel-permission": "^5.3",
        "voyager-admin/voyager": "2.x-dev"
    }
}
